{
    "parser": "babel-eslint",
      "parserOptions": {
      "ecmaVersion": 2017,
        "sourceType": "module",
          "ecmaFeatures": {
        "jsx": true,
          "arrowFunctions": true,
            "classes": true,
              "spread": true
      }
    },
    "rules": {
      "no-trailing-spaces": "warn",
        "eol-last": "warn"
    }
  }